<Window x:Class="Библиотека.Reg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Библиотека" d:DataContext="{d:DesignInstance Type=local:Reg}"
        mc:Ignorable="d"
        Title="Reg" Height="450" Width="400">
    <StackPanel>
        <TextBlock Text="Логин"/>
        <TextBox Text="{Binding Login}"/>
        <TextBlock Text="Пароль"/>
        <TextBox Text="{Binding Password}"/>
        <TextBlock Text="Имя"/>
        <TextBox Text="{Binding FIO}"/>
        <TextBlock Text="Телефон"/>
        <TextBox Text="{Binding Phone}"/>
        <Button Content="Отмена" Click="CancelButton"/>
        <Button Content="Зарегистрироваться" Click="RegButton"/>
    </StackPanel>
</Window>
